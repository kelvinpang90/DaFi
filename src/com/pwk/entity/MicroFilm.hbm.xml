<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.pwk.entity.MicroFilm" table="micro_film" catalog="dafi">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="title" type="java.lang.String">
            <column name="title" length="200" />
        </property>
        <property name="shortDescription" type="java.lang.String">
            <column name="short_description" length="500" />
        </property>
        <property name="description" type="java.lang.String">
            <column name="description"/>
        </property>
        <property name="icon" type="java.lang.String">
            <column name="icon" length="200"/>
        </property>
        <property name="pic" type="java.lang.String">
            <column name="pic"/>
        </property>
        <property name="video" type="java.lang.String">
            <column name="video"/>
        </property>
        <property name="position" type="java.lang.Integer">
            <column name="position"/>
        </property>
        <property name="projectTime" type="java.lang.String">
            <column name="project_time"/>
        </property>
        <property name="createTime" type="java.sql.Timestamp">
            <column name="create_time"/>
        </property>
    </class>
</hibernate-mapping>
